{
  "version": "3.0.0",
  "description": "Common spending/debit detection and amount extraction patterns for all banks and UPI apps",
  "commonPatterns": {
    "spendingKeywords": [
      "debited",
      "spent",
      "debit",
      "dr",
      "withdrawn",
      "payment made",
      "amount spent",
      "transaction",
      "purchase",
      "payment",
      "paid",
      "sent",
      "outgoing",
      "transfer sent",
      "deduction",
      "txn",
      "bill",
      "charge",
      "fee",
      "withdrawal",
      "upi debit",
      "imps debit",
      "neft debit",
      "rtgs debit",
      "spent on",
      "spent for",
      "amount debited",
      "amount spent",
      "payment made",
      "payment sent"
    ],

    "amountPatterns": {
      "INR": [
        "(?:Rs\\.?|INR|Rs|₹)\\s*([0-9,]+(?:\\.[0-9]+)?)",
        "([0-9,]+(?:\\.[0-9]+)?)\\s*(?:Rs\\.?|INR|Rs|₹)",
        "([0-9,]+(?:\\.[0-9]+)?)"
      ],
      "USD": [
        "(?:USD|US\\$|\\$)\\s*([0-9,]+(?:\\.[0-9]+)?)",
        "([0-9,]+(?:\\.[0-9]+)?)\\s*(?:USD|US\\$|\\$)"
      ],
      "EUR": [
        "(?:EUR|€|EURO)\\s*([0-9,]+(?:\\.[0-9]+)?)",
        "([0-9,]+(?:\\.[0-9]+)?)\\s*(?:EUR|€|EURO)"
      ],
      "GBP": [
        "(?:GBP|£|POUND)\\s*([0-9,]+(?:\\.[0-9]+)?)",
        "([0-9,]+(?:\\.[0-9]+)?)\\s*(?:GBP|£|POUND)"
      ],
      "CAD": [
        "(?:CAD|C\\$|CAN\\$)\\s*([0-9,]+(?:\\.[0-9]+)?)",
        "([0-9,]+(?:\\.[0-9]+)?)\\s*(?:CAD|C\\$|CAN\\$)"
      ],
      "AUD": [
        "(?:AUD|A\\$|AUS\\$)\\s*([0-9,]+(?:\\.[0-9]+)?)",
        "([0-9,]+(?:\\.[0-9]+)?)\\s*(?:AUD|A\\$|AUS\\$)"
      ],
      "JPY": [
        "(?:JPY|¥|YEN)\\s*([0-9,]+(?:\\.[0-9]+)?)",
        "([0-9,]+(?:\\.[0-9]+)?)\\s*(?:JPY|¥|YEN)"
      ],
      "CHF": [
        "(?:CHF|FRANC)\\s*([0-9,]+(?:\\.[0-9]+)?)",
        "([0-9,]+(?:\\.[0-9]+)?)\\s*(?:CHF|FRANC)"
      ],
      "CNY": [
        "(?:CNY|¥|RMB)\\s*([0-9,]+(?:\\.[0-9]+)?)",
        "([0-9,]+(?:\\.[0-9]+)?)\\s*(?:CNY|¥|RMB)"
      ],
      "HKD": [
        "(?:HKD|HK\\$)\\s*([0-9,]+(?:\\.[0-9]+)?)",
        "([0-9,]+(?:\\.[0-9]+)?)\\s*(?:HKD|HK\\$)"
      ],
      "NZD": [
        "(?:NZD|NZ\\$)\\s*([0-9,]+(?:\\.[0-9]+)?)",
        "([0-9,]+(?:\\.[0-9]+)?)\\s*(?:NZD|NZ\\$)"
      ]
    },
